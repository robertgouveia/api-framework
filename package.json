    "@types/node": "^22.13.14",
    "@types/pg": "^8.11.11",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1"
  }
}
